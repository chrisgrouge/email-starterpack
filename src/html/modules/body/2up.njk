<!-- START: MODULE - 2-UP -->
<tr>
  <th class="pad-btm--60" border="0" style="width:100%;" width="100%" align="center">
    <table inline-class="mobile" role="presentation" style="width: 100%;" width="100%" cellspacing="0" cellpadding="0" border="0" align="center">
      <tr>
        <!-- col 1 -->
        <th class="pad-r--10" inline-class="m-width--50 m-pad-r--0" border="0" style="width: 320px; vertical-align: top;" width="320" align="center">
          <table inline-class="mobile" role="presentation" style="width: 100%;" width="100%" cellspacing="0" cellpadding="0" border="0" align="center">
