<!-- START: MODULE - BACKGROUND IMAGE TOP -->
<tr>
  <th class="pad-btm--60" align="center" style="width: 100%;" width="100%">
    <table inline-class="mobile" align="center" width="100%" role="presentation" border="0" cellpadding="0" cellspacing="0" style="width: 100%;">
      <tr>
        <th inline-class="m-background-top" background="https://via.placeholder.com/650x742" width="650" height="742" style="background-image: url('https://via.placeholder.com/650x742'); background-repeat: no-repeat; background-position: center center; background-size: cover; background-color: #cccccc; vertical-align: top;">
          <!-- convert pixels to points for outlook width and height using the expectation that 1px = 1pt -->
          <!-- formula: pt = px * .75 -->
          <!--[if gte mso 9]>
            <v:image xmlns:v="urn:schemas-microsoft-com:vml" fill="true" stroke="false" style="border: 0;display: inline-block; width:487.5pt; height:556.5pt;" src="https://via.placeholder.com/650x742" />
            <v:rect xmlns:v="urn:schemas-microsoft-com:vml" fill="true" stroke="false" style="border: 0;display: inline-block;position: absolute; width:484.5pt; height:553.5pt;">
            <v:fill  opacity="0%" color="#cccccc"  />
            <v:textbox style="v-text-align: top" inset="0,0,0,0">
            <![endif]-->
          <div>
            <!-- Overlay image or text -->
            <table align="center" border="0" cellpadding="0" cellspacing="0" style="width: 100%; vertical-align: top;" width="100%" role="presentation">
              <tr>
                <th inline-class="m-background-topHeight" height="742" style="width: 100%; height: 742px; vertical-align: top;">
                  <table inline-class="mobile" align="center" width="100%" role="presentation" border="0" cellpadding="0" cellspacing="0" style="width: 100%;">
                    <tr>
                      <!-- inner spacer 1 -->
                      <th align="center" width="30" border="0" style="width: 30px; min-width: 30px;">&nbsp;</th>
                      <!-- text -->
                      <th class="pad-tb--35" align="center" border="0">
                        <table inline-class="mobile" align="center" width="100%" role="presentation" border="0" cellpadding="0" cellspacing="0" style="width: 100%;">
                          <!-- headline h1 and paragraph -->
                          <tr>
                            <th class="pad-btm--25" align="center" width="100%" border="0" style="width: 100%;">
                              <h1 class="txt--h1 marg-btm--15 white" inline-class="txt--h1">LARGE HEADLINE HERE</h1>
                              <p class="txt--body white" inline-class="txt--body">Lorem ipsum dolor sit amet, consectetur adipiscing elit. Donec vel est <br class="hide" />odio. Quisque a mollis metus. Pellentesque egestas, lectus sit amet.</p>
                            </th>
                          </tr>
                          <!-- button -->
                          {% include "modules/partials/button-light.njk" %}
                        </table>
                      </th>
                      <!-- inner spacer 2 -->
                      <th align="center" width="30" border="0" style="width: 30px; min-width: 30px;">&nbsp;</th>
                    </tr>
                  </table>
                </th>
              </tr>
            </table>
          </div>
          <!--[if gte mso 9]>
                  </v:textbox>
                  </v:fill>
                  </v:rect>
                  </v:image>
                  <![endif]-->
        </th>
      </tr>
    </table>
  </th>
</tr>
<!-- END: MODULE - BACKGROUND IMAGE TOP -->